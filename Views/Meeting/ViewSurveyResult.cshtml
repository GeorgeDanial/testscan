                    @if (!string.IsNullOrEmpty(Request.QueryString["Participate"]))
                    {
                        @Html.ActionLink("Back to Participate", "View", "Participate", new { Id = Request.QueryString["Participate"] }, new { @class = "btn btn-danger pull-right mr-2" })
                    }

