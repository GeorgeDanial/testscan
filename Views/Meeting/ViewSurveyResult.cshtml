                                <tbody>
                                    <tr>
                                        <th style="width:50%">Participant Name :</th>
                                        <td>@Model.Name</td>
                                    </tr>
                                    <tr>
                                        <th style="width:50%">Participant Email :</th>
                                        <td>@Model.Email</td>
                                    </tr>
                                    <tr>
                                        <th style="width:50%">Submitted Date :</th>
                                        <td>@Model.SubmitDate</td>
                                    </tr>
                                    <tr>
                                        <th style="width:50%">Reminder Date :</th>
                                        <td>@Model.ReminderDate</td>
                                    </tr>
                                    <tr>
                                        <th style="width:50%">Network ID :</th>
                                        <td>@Model.TypeFormNetworkId</td>
                                    </tr>
                                    <tr>
                                        <th style="width:50%">   Effective Raw Score : </th>
                                        <td>@Model.MeetingScore</td>
                                    </tr>
