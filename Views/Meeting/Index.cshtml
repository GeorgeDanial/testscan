                    <div id="webGrid_wrapper" class="dataTables_wrapper container-fluid dt-bootstrap4 no-footer">
                        @Html.Partial("_KoGrid")

                    </div>

                </div>

            </div>
        </div>
    </div>
</div>

@section Styles {
